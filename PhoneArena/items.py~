# Define here the models for your scraped items
#
# See documentation in:
# http://doc.scrapy.org/en/latest/topics/items.html

from scrapy.item import Item, Field

class PhonearenaItem(Item):
    # define the fields for your item here like:
    # name = Field()
    DeviceType = Field()
    OS = Field()
    Dimensions = Field()
    Weight = Field()
    Size= Field()
    Resolution = Field()
    Touchscreen = Field()
    Chip = Field()
    Processor = Field()
    GraphicsProcessor = Field()
    SystemMemory = Field()
    StorageExpansion = Field()
    attributes = Field()
    values = Field ()
    pass
